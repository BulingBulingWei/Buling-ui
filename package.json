{
  "name": "buling-ui",
  "private": true,
  "version": "0.0.0",
  "description": "三端统一Vue3组件库",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:site": "cd site && vite --host",
    "gen": "node scripts/generator",
    "gen:api": "node scripts/generateAPIMd",
    "build": "vue-tsc -b && vite build",
    "build:site": "cd site && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "title": "Buling-Ui",
  "main": "es/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js"
    },
    "./*": "./*"
  },
  "dependencies": {
    "@buling-ui-icon": "link:..\\buling-ui-icon",
    "@vueuse/core": "^11.2.0",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.8",
    "swiper": "^11.1.14",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "echarts": "^5.5.1",
    "enquirer": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "execa": "^9.5.1",
    "globals": "^15.12.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "ora": "^8.1.1",
    "postcss": "^8.4.47",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^2.8.8",
    "prismjs": "^1.29.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tdesign-site-components": "^0.15.4",
    "tdesign-vue-next": "^1.10.3",
    "typescript": "~5.6.2",
    "vconsole": "^3.15.1",
    "vite": "^5.4.10",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-tdoc": "^2.0.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.1.8"
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "maintainers": [
    "wei"
  ]
}
